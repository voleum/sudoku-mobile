<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Method: app-store для TestFlight и App Store -->
    <key>method</key>
    <string>app-store</string>

    <!-- Team ID: замените на ваш Apple Developer Team ID -->
    <!-- Найти можно в Apple Developer Account → Membership -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>

    <!-- Upload Symbols: включить для crash reporting -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Bitcode: отключен для React Native приложений -->
    <key>uploadBitcode</key>
    <false/>
    <key>compileBitcode</key>
    <false/>

    <!-- Provisioning Profile: автоматическое управление -->
    <!-- Xcode автоматически выберет правильный provisioning profile -->
    <key>provisioningProfiles</key>
    <dict>
        <!-- Bundle ID приложения: замените на ваш -->
        <key>com.sudoku</key>
        <string>match AppStore com.sudoku</string>
    </dict>

    <!-- Signing Style: automatic для упрощения -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- Strip Swift Symbols: да, для уменьшения размера -->
    <key>stripSwiftSymbols</key>
    <true/>

    <!-- Thinning: app-store для оптимизации размера -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
</dict>
</plist>
